<div class="modal fade" id="modalAddSpace" tabindex="-1" role="dialog" aria-labelledby="addSpaceModalLabel"
     aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            {{ form_start(form, {'action': path('schedule_space_insert')}) }}
            <div class="modal-header">
                <h5 class="modal-title" id="addSpaceModalLabel">New Space</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>

            <div class="modal-body">
                {{ form_row(form.schedule) }}
                {{ form_row(form.type) }}
                {{ form_row(form.visible) }}
                {{ form_row(form.name) }}
            </div>

            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                <button class="btn btn-primary">Save new space</button>
            </div>
            {{ form_end(form) }}
        </div>
    </div>
</div>